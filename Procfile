caddy: /opt/homebrew/bin/caddy run -c conf/Caddyfile
redis: /opt/homebrew/opt/redis@6.2/bin/redis-server /opt/homebrew/etc/redis.conf
mysql: /opt/homebrew/opt/mysql@8.0/bin/mysqld_safe --datadir=/opt/homebrew/var/mysql
php82: /opt/homebrew/opt/php@8.2/sbin/php-fpm -y conf/phpfpm82.conf -F
php83: /opt/homebrew/opt/php@8.3/sbin/php-fpm -y conf/phpfpm83.conf -F
# php83x: /opt/homebrew/opt/php@8.3/sbin/php-fpm -y conf/phpfpm83.xdebug.conf -F -d zend_extension=xdebug.so -d xdebug.mode=debug,profile -d xdebug.start_with_request=yes
# clickhouse: /opt/homebrew/bin/clickhouse server --config conf/clickhouse.xml
