caddy: /opt/homebrew/bin/caddy run -c conf/Caddyfile
redis: /opt/homebrew/opt/redis/bin/redis-server /opt/homebrew/etc/redis.conf
mysql: /opt/homebrew/opt/mysql/bin/mysqld_safe --datadir=/opt/homebrew/var/mysql
php81: /opt/homebrew/opt/php@8.1/sbin/php-fpm -y conf/phpfpm81.conf -F
php82: /opt/homebrew/opt/php@8.2/sbin/php-fpm -y conf/phpfpm82.conf -F
