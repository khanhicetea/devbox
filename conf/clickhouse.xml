<?xml version="1.0"?>
<clickhouse>
    <logger>
        <level>notice</level>
        <log>/opt/homebrew/var/clickhouse/clickhouse-server.log</log>
        <errorlog>/opt/homebrew/var/clickhouse/clickhouse-server.err.log</errorlog>
    </logger>
    <http_port>8123</http_port>
    <tcp_port>9123</tcp_port>
    <listen_host>127.0.0.1</listen_host>
    <path>/opt/homebrew/var/clickhouse/</path>
    <profiles>
        <default>
            <max_memory_usage>3000000000</max_memory_usage>
            <max_threads>2048</max_threads>
        </default>
        </profiles>
    <user_directories>
        <users_xml>
            <path>clickhouse_users.xml</path>
        </users_xml>
    </user_directories>
</clickhouse>
